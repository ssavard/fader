<% provide(:title, @market.name) %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= @market.name %>
        <%= @market.rank %>
        <%= @market.description %>
      </h1>
      <%= button_to "Edit", edit_market_path(@market), :class => "btn btn-primary", :method => :get %>
    </section>
  </aside>
</div>